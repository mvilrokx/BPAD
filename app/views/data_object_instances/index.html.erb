<% title "Data Object Instances" %>

<table>
  <tr>
    <th>Name</th>
    <th>Step</th>
    <th>Business Process Element</th>
    <th>Iteration</th>
  </tr>
  <% for data_object_instance in @data_object_instances %>
    <tr>
      <td><%=h data_object_instance.name %></td>
      <td><%=h data_object_instance.step_id %></td>
      <td><%=h data_object_instance.business_process_element_id %></td>
      <td><%=h data_object_instance.iteration_id %></td>
      <td><%= link_to "Show", data_object_instance %></td>
      <td><%= link_to "Edit", edit_data_object_instance_path(data_object_instance) %></td>
      <td><%= link_to "Destroy", data_object_instance, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Data Object Instance", new_data_object_instance_path %></p>
