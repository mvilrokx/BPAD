<% object_name = objects[0].class.name.humanize.pluralize %>
<% new||=false %>
<% title object_name, false %>
<h3>
	<%= object_name %>
	<% if current_user.iteration && new %>
		<%= link_to image_tag('/icons/add.png', :alt=> 'Add New #{object_name}') , new_object_path, :title => 'Add New #{object_name}' %>
	<% end %>
</h3>
